pipeline {  
    agent any  
        stages {  
       	    stage("git_checkout Developer1") {  
           	    steps {  
              	    echo "cloning repository" 
              	    echo "repo cloned successfully"  
              	    }  
         	    } 
				
				stage("Build") {  
           	    steps {  
              	    echo "build the project" 
              	    echo "repo build successfully"  
              	    }  
         	    } 
				
				stage("Test") {  
           	    steps {  
              	    echo "build the project" 
              	    echo "repo build successfully"  
              	    }  
         	    } 
				

        }
}
